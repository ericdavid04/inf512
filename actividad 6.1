using System.Globalization;
using Internal;
using System;
using System.Threading;
using System.Collections.Generic;

public class Empresa
{
    public int Id { get; set; }
    public string Nombre { get; set; }
    public List<Cliente> Clientes { get; set; }
    
    public Empresa()
    {
        Clientes = new List<Cliente>();
    }
}

public class Cliente
{
    public int Id { get; set; }
    public string Nombre { get; set; }
    public int Edad { get; set; }
}

public class Program
{
    static void Main(string[] args)
    {
        Empresa empresa = new Empresa();
        empresa.Id = 1;
        empresa.Nombre = "APP SOLUTION";

        Cliente cliente1 = new Cliente();
        cliente1.Id = 1;
        cliente1.Nombre = "Horacio Vasquez";
        cliente1.Edad = 30;

        Cliente cliente2 = new Cliente();
        cliente2.Id = 2;
        cliente2.Nombre = "Olfa Ramirez";
        cliente2.Edad = 40;

        empresa.Clientes.Add(cliente1);
        empresa.Clientes.Add(cliente2);

        Console.WriteLine("La empresa " + empresa.Nombre + " tiene " + empresa.Clientes.Count + " clientes:");
        foreach (var cliente in empresa.Clientes)
        {
            Console.WriteLine(" - " + cliente.Nombre + " (" + cliente.Edad + " años)");
        }

        Console.ReadLine();
    }
}
